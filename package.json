  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "biome check --write --unsafe"
    ]
  },
  "name": "reactsquad-react-router-saas-template",
  "packageManager": "pnpm@10.29.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "esbuild",
      "lefthook",
      "msw",
      "prisma",
      "sharp"
    ]
  },
  "private": true,
  "scripts": {
    "build": "react-router build",
    "check": "biome check --write .",
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router dev",
    "dev:mocks": "cross-env MOCKS=true NODE_OPTIONS='--import ./instrument.server.mjs' pnpm run dev",
    "lint": "biome ci --css-parse-tailwind-directives=true .",
    "prepare": "lefthook install",
    "prisma:deploy": "pnpm exec prisma migrate deploy && pnpm exec prisma generate",
    "prisma:migrate": "pnpm exec prisma migrate dev --name",
    "prisma:push": "pnpm exec prisma db push && pnpm exec prisma generate",
    "prisma:reset-dev": "pnpm run prisma:wipe && pnpm run prisma:seed && pnpm run dev",
    "prisma:seed": "tsx ./prisma/seed.ts",
    "prisma:setup": "prisma generate && prisma migrate deploy && prisma db push",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:wipe": "pnpm exec prisma migrate reset --force && pnpm exec prisma db push",
    "quickstart": "cp .env.example .env && pnpm run prisma:setup && pnpm run prisma:seed && pnpm run dev:mocks",
    "start": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router-serve ./build/server/index.js",
    "start:mocks": "cross-env MOCKS=true NODE_OPTIONS='--import ./instrument.server.mjs' pnpm run start",
    "stripe:resend-events": "xargs -n1 stripe events resend < stripe-events.txt",
    "supabase:deploy-functions": "supabase functions deploy --project-ref $SUPABASE_PROJECT_ID",
    "supabase:serve-functions": "supabase functions serve",
    "test": "vitest --reporter=verbose --run",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:ui": "pnpm exec playwright test --ui",
    "test:watch": "vitest --reporter=verbose",
    "typecheck": "pnpm run typegen && tsc",
    "typegen": "pnpm run typegen:router && pnpm run typegen:prisma",
    "typegen:prisma": "prisma generate",
    "typegen:router": "react-router typegen"
  },
  "type": "module"
}
